# Base URL for OAuth callbacks
BASE_URL=http://localhost:8000

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# GitHub OAuth
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Facebook OAuth
FACEBOOK_CLIENT_ID=your-facebook-client-id
FACEBOOK_CLIENT_SECRET=your-facebook-client-secret

# App Configuration
APP_PAGE_SIZE=10
JWT_SECRET=your-jwt-secret-key
PREFIX_URL=http://127.0.0.1:8000
RUNTIME_ROOT_PATH=runtime/

# Image Upload Configuration
IMAGE_SAVE_PATH=upload/images/
IMAGE_MAX_SIZE=5
IMAGE_ALLOW_EXTS=.jpg,.jpeg,.png

# Export and File Paths
EXPORT_SAVE_PATH=export/
QR_CODE_SAVE_PATH=qrcode/
FONT_SAVE_PATH=fonts/

# Logging Configuration
LOG_SAVE_PATH=logs/
LOG_SAVE_NAME=log
LOG_FILE_EXT=log
TIME_FORMAT=20060102

# Server Configuration
RUN_MODE=debug
HTTP_PORT=8000
READ_TIMEOUT=60
WRITE_TIMEOUT=60

# Database Configuration
DB_TYPE=mysql
DB_USER=root
DB_PASSWORD=your-db-password
DB_HOST=127.0.0.1:3306
DB_NAME=blog
DB_TABLE_PREFIX=blog_

# Redis Configuration
REDIS_HOST=127.0.0.1:6379
REDIS_PASSWORD=
REDIS_MAX_IDLE=30
REDIS_MAX_ACTIVE=30
REDIS_IDLE_TIMEOUT=200